{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)", 
      "dest": "/Public/$1"
    },
    {
      "src": "/login", 
      "dest": "/server.js"
    },
    {
      "src": "/register", 
      "dest": "/server.js"
    },
    {
      "src": "/lego/sets", 
      "dest": "/server.js"
    },
    {
      "src": "/(.*)", 
      "dest": "/server.js"
    }
  ]
}
