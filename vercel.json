{
  "version": 2,
  "builds": [
    {
      "src": "server.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Modules/api/$1.js" 
    },
    {
      "src": "/(.*)",
      "dest": "Public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "views/$1"
    }
}
